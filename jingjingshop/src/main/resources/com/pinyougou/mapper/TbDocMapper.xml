  <select id="selectPurchaseRecord" resultType="java.util.Map" parameterType="Long">
         SELECT
			t.order_id orderId,
			t.payment,
			t.`status` orderStatus,
			t.create_time createTime,
			t.user_id userId,
		  (select t3.name from tb_seller t3 where t3.seller_id = t.seller_id)sellerName,
